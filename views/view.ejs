<% const emp=data.employees[0] %>

<% const storedetail=data.storedetails %>
<nav
style="width: 88%"
class="px-2 mt-4 d-flex flex-wrap justify-content-between"
aria-label="breadcrumb"
>
<ol class="breadcrumb">
  <li class="breadcrumb-item"><a href="/dash">Back</a></li>
  <li class="breadcrumb-item active" aria-current="page">
    <%= emp.employee_name +"  "+emp.employee_lastname  %>
  </li>
</ol>

<p class=" ">
  <span class="fw-medium fs-6">Id :</span> <%= emp.employee_id %>
</p>
</nav>


<table
style="width: 88%"-
class="ms-1 table mt-5 table-striped table-bordered"
>
<thead>
  <tr>
    <th scope="col">First Name</th>
    <th class="fw-normal" scope="col"><%= emp.employee_name %></th>
  </tr>
</thead>
<tbody>
  <tr>
    <th scope="row">Last Name</th>
    <td><%= emp.employee_lastname %></td>
  </tr>

  <tr>
    <th scope="row">Telephone</th>
    <td><%= emp.employee_phone %></td>
  </tr>

  <tr>
    <th scope="row">Email</th>
    <td><%= emp.employee_email %></td>
  </tr>

  <tr>
    <th scope="row">Gender</th>
    <td><%= emp.employee_gender %></td>
  </tr>

  <tr>
    <th scope="row">Role</th>
    <td><%= emp.employee_role %></td>
  </tr>

  <tr>
    <th scope="row">Address</th>
    <td><%= emp.employee_address %></td>
  </tr>
  <tr>
    
    <th scope="row">Store</th>

    <% const storeData = storedetail.find(element => emp.storeID === element.StoreID); %>
    <td><%= storeData ? storeData.StoreName : 'Store not found' %></td>
  
  </tr>

</tbody>
</table>
